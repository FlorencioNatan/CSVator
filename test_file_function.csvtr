soma :: int a, int b -> int;
soma: a + b;

soma(5, 6);

soma(12, 75);

subMultESoma :: int a, int b -> int;
subMultESoma: (a * b) - (a + b);

subMultESoma(10, 10);


somaOuSubtrai :: int a, int b -> int;
somaOuSubtrai [a < b] : a + b;
somaOuSubtrai [a > b] : a - b;
somaOuSubtrai: 2;


somaOuSubtrai(2, 5);
somaOuSubtrai(5, 2);
somaOuSubtrai(5, 5);


constante ::-> int;
constante: 9;

constante();


repeticao :: int a, int b, int i -> int;
repeticao [i < 5] : repeticao(b * i + a, b * 10, i + 1);
repeticao [i > 5] : a;

